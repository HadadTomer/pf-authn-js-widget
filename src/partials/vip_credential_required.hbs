<div class="card">
  <div class="org-logo">
    <img class="org-logo__image" src="{{logo}}" alt="Company Logo"/>
  </div>
  {{> errors}}
  <h1 class="heading">{{username}}</h1>
  <div class="text-block">
    <div class="text-block__text">Add Credential.</div>
  </div>
  <div class="tile-selector tile-selector--stacked">
    <form class="form form--margin-small" id="AuthnWidgetForm">
      <div class="stack" data-id="stack">
        <div class="float-label">
          <input id="vipCredentialId" type="text" class="text-input float-label__input required" name="vipCredentialId" required/>
          <label class="float-label__label" for="vipCredentialId">Credential ID</label>
        </div>
        <div class="float-label">
          <input id="securityCode" type="text" class="text-input float-label__input required" name="securityCode" required/>
          <label class="float-label__label" for="securityCode">Security Code</label>
        </div>
        <div class="text-block">
          <button id="submit" type="submit" class="button button--primary brand-primary-bg" data-id="button"
                  data-actionId="submitVIPCredential" disabled>Submit
          </button>
        </div>
        <div class="text-block">
          <a tabindex="0" data-actionId="cancel" target="_self" class="anchor">Cancel</a>
        </div>
      </div>
    </form>
  </div>
</div>